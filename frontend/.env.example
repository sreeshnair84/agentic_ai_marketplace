# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000

# A2A Configuration
# Set to 'true' to use mock A2A service instead of real backend (useful for development)
NEXT_PUBLIC_USE_MOCK_A2A=false

# Legacy service URLs (optional - used for fallback routing)
NEXT_PUBLIC_AGENTS_URL=http://localhost:8002
NEXT_PUBLIC_ORCHESTRATOR_URL=http://localhost:8003

# Copy from .env.local on the Vercel dashboard
# https://nextjs.org/learn/dashboard-app/setting-up-your-database#create-a-postgres-database
POSTGRES_URL=
POSTGRES_PRISMA_URL=
POSTGRES_URL_NON_POOLING=
POSTGRES_USER=
POSTGRES_HOST=
POSTGRES_PASSWORD=
POSTGRES_DATABASE=

# `openssl rand -base64 32`
AUTH_SECRET=
AUTH_URL=http://localhost:3000/api/auth

# Other optional services
REDIS_URL=redis://localhost:6379